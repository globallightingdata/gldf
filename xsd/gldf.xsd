										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="TemperatureClass" maxOccurs="14">
													<xs:annotation>
														<xs:documentation>Temperature classification</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="T1"/>
															<xs:enumeration value="T2"/>
															<xs:enumeration value="T2A"/>
															<xs:enumeration value="T2B"/>
															<xs:enumeration value="T2C"/>
															<xs:enumeration value="T2D"/>
															<xs:enumeration value="T3"/>
															<xs:enumeration value="T3A"/>
															<xs:enumeration value="T3B"/>
															<xs:enumeration value="T3C"/>
															<xs:enumeration value="T4"/>
															<xs:enumeration value="T4A"/>
															<xs:enumeration value="T5"/>
															<xs:enumeration value="T6"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="ExCodes" minOccurs="0">
