												<xs:enumeration value="Interior: Hospitals and Care Places: Health Care Patient Rooms"/>
												<xs:enumeration value="Interior: Hospitals and Care Places: Health Care Clean Room Areas"/>
												<xs:enumeration value="Interior: Hospitals and Care Places: Health Care Examination Rooms"/>
