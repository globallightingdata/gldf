												<xs:enumeration value="Metal Halide (250/400W)"/>
												<xs:enumeration value="Ceramic Metal Halide (50/150W)"/>
												<xs:enumeration value="High Pressure Sodium (250/400W)"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="CieLampMaintenanceFactors">
