														<xs:restriction base="xs:string">
															<xs:enumeration value="I"/>
															<xs:enumeration value="II"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="EquipmentCategories" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Defines in which zones the equipment can be used. With protection level from normal to high</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="EquipmentCategory" maxOccurs="8">
