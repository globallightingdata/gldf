													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="ATEX"/>
															<xs:enumeration value="IECEx"/>
															<xs:enumeration value="CEC"/>
															<xs:enumeration value="NEC"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
