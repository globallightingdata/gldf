												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="DivisionGroups" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Hazardous atmosphere category groups A-G</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Gas" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Gas, fog, steam zones</xs:documentation>
													</xs:annotation>
