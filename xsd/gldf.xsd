																<xs:restriction base="xs:int">
																	<xs:minInclusive value="0"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="LedMaintenanceFactor">
