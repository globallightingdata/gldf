														<xs:documentation>Possible equipment hazardous zone. Mining, Gas, Dust and corresponding protection level</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="M1"/>
															<xs:enumeration value="M2"/>
															<xs:enumeration value="1G"/>
															<xs:enumeration value="2G"/>
															<xs:enumeration value="3G"/>
															<xs:enumeration value="1D"/>
															<xs:enumeration value="2D"/>
															<xs:enumeration value="3D"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Atmospheres" minOccurs="0">
