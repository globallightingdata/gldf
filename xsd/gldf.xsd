					<xs:documentation>Descriptive attributes for marketing</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ListPrices" minOccurs="0">
							<xs:annotation>
								<xs:documentation>List of prices per unit</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ListPrice" maxOccurs="unbounded">
